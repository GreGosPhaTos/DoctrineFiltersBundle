{
    "name": "gregosphatos/doctrine-filters-bundle",
    "description": "Some nice doctrine filters",
    "keywords": ["symfony2", "doctrine2", "filters"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "Adrien Petitjean",
        "email" : "adrien.petitjean84@gmail.com"
    }],
    "require": {
        "php" : ">=5.3.0",
        "doctrine/common": "~2.4",
        "doctrine/mongodb-odm": "~1.0.0-beta10@dev",
        "doctrine/orm": "~2.2,>=2.2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
